      </Label>
      <TextField fx:id="inputEquipamento" layoutX="42.0" layoutY="160.0" prefHeight="25.0" prefWidth="276.0" />
      <Label layoutX="42.0" layoutY="218.0" text="Detalhe o problema">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TextArea fx:id="inputDescricao" layoutX="42.0" layoutY="264.0" prefHeight="115.0" prefWidth="327.0" />
      <Label layoutX="249.0" layoutY="14.0" text="Abra um novo chamado">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <Button fx:id="okButton" layoutX="375.0" layoutY="558.0" mnemonicParsing="false" onAction="#cadastrarChamado" prefHeight="25.0" prefWidth="69.0" text="Pronto">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="backButton" layoutX="16.0" layoutY="14.0" mnemonicParsing="false" onAction="#switchWindow" text="Voltar" />
